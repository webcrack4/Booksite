@model IEnumerable<BookSite.ViewModel.BookGroup>

@{
    ViewBag.Title = "Book Number";
}

<h2>User Number</h2>

<table>
    <tr>
        <th>
            User Type
        </th>
        <th>
            Number
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @item.BookCount
            </td>
        </tr>
    }
</table>